<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object class="GtkImage" id="start_stop_image">
        <property name="icon-name">media-playback-pause-symbolic</property>
    </object>
    <object class="GtkListBoxRow" id="row">
        <child>
            <object class="GtkBox">
                <child type="center">
                    <object class="GtkLabel" id="label">
                        <style>
                            <class name="timer-label"/>
                        </style>
                    </object>
                </child>
                <child>
                    <object class="GtkButton" id="start_stop_button">
                        <property name="image">start_stop_image</property>
                    </object>
                    <packing>
                        <property name="pack_type">end</property>
                        <property name="expand">false</property>
                        <property name="fill">false</property>
                    </packing>
                </child>
            </object>
        </child>
    </object>
</interface>
